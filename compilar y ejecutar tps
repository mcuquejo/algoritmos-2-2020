--STRUTIL
gcc -g -std=c99 -Wall -Wconversion -Wtype-limits -pedantic -Werror -o strutil strutil.c
valgrind --leak-check=full --track-origins=yes --show-reachable=yes -s ./strutil
----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

--POSTFIX
gcc -g -std=c99 -Wall -Wconversion -Wtype-limits -pedantic -Werror -o dc dc.c pila.c strutil.c
valgrind --leak-check=full --track-origins=yes --show-reachable=yes -s ./dc < oper.txt
----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

--INFIX
gcc -g -std=c99 -Wall -Wconversion -Wtype-limits -pedantic -Werror -o infix infix.c pila.c cola.c strutil.c
valgrind --leak-check=full --track-origins=yes --show-reachable=yes -s ./infix < arith.txt
----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
--HASH
gcc -g -std=c99 -Wall -Wconversion -Wtype-limits -pedantic -Werror -o hash hash.c lista.c testing.c hash_pruebas.c main.c
valgrind --leak-check=full --track-origins=yes --show-reachable=yes -s ./hash

--asi compila el corrector
gcc -g -O2 -std=c99 -Wall -Werror -pedantic -Wformat=2 -Wshadow -Wpointer-arith -Wunreachable-code -Wconversion -Wno-sign-conversion -Wbad-function-cast -DCORRECTOR   -c -o pruebas_catedra.o pruebas_catedra.c
gcc -g -O2 -std=c99 -Wall -Werror -pedantic -Wformat=2 -Wshadow -Wpointer-arith -Wunreachable-code -Wconversion -Wno-sign-conversion -Wbad-function-cast -DCORRECTOR   -c -o testing.o testing.c
gcc -g -O2 -std=c99 -Wall -Werror -pedantic -Wformat=2 -Wshadow -Wpointer-arith -Wunreachable-code -Wconversion -Wno-sign-conversion -Wbad-function-cast -DCORRECTOR   -c -o lista.o lista.c
gcc -g -O2 -std=c99 -Wall -Werror -pedantic -Wformat=2 -Wshadow -Wpointer-arith -Wunreachable-code -Wconversion -Wno-sign-conversion -Wbad-function-cast -DCORRECTOR   -c -o main.o main.c
gcc -g -O2 -std=c99 -Wall -Werror -pedantic -Wformat=2 -Wshadow -Wpointer-arith -Wunreachable-code -Wconversion -Wno-sign-conversion -Wbad-function-cast -DCORRECTOR   -c -o hash.o hash.c
----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
--ABB
gcc -g -std=c99 -Wall -Wconversion -Wtype-limits -pedantic -Werror -o abb abb.c testing.c pruebas_alumno.c main.c
valgrind --leak-check=full --track-origins=yes --show-reachable=yes -s ./abb
----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------