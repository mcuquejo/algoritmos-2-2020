--STRUTIL
gcc -g -std=c99 -Wall -Wconversion -Wtype-limits -pedantic -Werror -o strutil strutil.c
valgrind --leak-check=full --track-origins=yes --show-reachable=yes -s ./strutil


--POSTFIX
gcc -g -std=c99 -Wall -Wconversion -Wtype-limits -pedantic -Werror -o dc dc.c pila.c strutil.c
valgrind --leak-check=full --track-origins=yes --show-reachable=yes -s ./dc < oper.txt


--INFIX
gcc -g -std=c99 -Wall -Wconversion -Wtype-limits -pedantic -Werror -o infix infix.c pila.c cola.c strutil.c
valgrind --leak-check=full --track-origins=yes --show-reachable=yes -s ./infix < arith.txt

--HASH
gcc -g -std=c99 -Wall -Wconversion -Wtype-limits -pedantic -Werror -o hash hash.c lista.c testing.c hash_pruebas.c main.c
valgrind --leak-check=full --track-origins=yes --show-reachable=yes -s ./hash